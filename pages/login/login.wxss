.container {
    height: 100vh;
    background-size: cover;
    background-repeat: no-repeat;
    /* width: 100vw; */
    background-image: url(data:image/jpg;base64,);
    overflow: hidden;
}
.header {
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    display: block;
    margin: 100rpx auto;
}
.login {
    width: 500rpx;
    height: 500rpx;
    margin: 0 auto;
    display: flex;
    flex-direction: column;
    background: #fff;
    padding: 30rpx;
    border-radius: 15rpx;
    opacity: .8;
    /* justify-content: space-between; */
}
.login view {
    flex: 1;
    margin: 10rpx 0;
    display: flex;
}
.account, .password {
    border-bottom: 1px solid tomato;
    flex: 1;
    margin: 0 20rpx;
}
.space_between {
    justify-content: space-between;
}
.loginBtn, .registerBtn {
    width: 100% !important;
    border-radius: 40rpx;
}